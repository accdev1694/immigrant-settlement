{
    "env": {
      "browser": true,
      "node": true,
      "es2022": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "globals": {
      "htmx": "readonly"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  }